#include <iostream>
int main ()
{
     int var,var1; 
    printf("Enter var  ");
    scanf("%d",&var);
    printf("Enter var  ");
    scanf("%x",&var1); 
    printf("This is var in 8  %o\n",var); 
    printf("This is var in 16 %x\n",var >> 2); 
    printf("Negation %o\n",~var); // побитовая операция отрицания 
    printf( " and %x, or %x, iskl or %x",var1 & var,var1 || var, var1 ^ var); // и или искл или 
    return 0;
 }
