#include <stdio.h>
#include <string.h>
int main() {
    //Number 2
    int limit=499;
    int pay,lim,mun,sum;
    printf("PAY ON LIMIT ");
    scanf("%d",&pay);
    printf("PAY OVER LIMIT");
    scanf("%d",&lim);
    printf("ALL MIN");
    scanf("%d",&mun);
    if(mun>limit){
        mun=mun-limit;
        sum=pay+mun*lim;
        printf("ITOG %d",sum);
        printf("\n");
    }
    else{
        printf("ITOG %d",pay);
    }
    //Number 3
    int var;
    printf("Number\n");
    scanf("%d",&var);
    if(var==0)
        printf("zero");
    if(var==1)
        printf("one");
    if(var==2)
        printf("two");
    if(var==3)
        printf("tree");
    if(var==4)
        printf("four");
    if(var==5)
        printf("five");
    if(var==6)
        printf("six");
    if(var==7)
        printf("seven");
    if(var==8)
        printf("eight");
    if(var==9)
        printf("nine");

    return 0;
}


